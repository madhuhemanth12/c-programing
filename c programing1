#include<stdio.h>
//function to input elements in a matrix
void inputmatrix(int mat[][3]){
for(int i=0;i<3;++i){
for(int j=0;j<3;++j){
printf("Enter element at position[%d][%d]:",i+1,j+1);
scanf("%d,&mat[i][j]);
}
}
}
//function to print a matrix 
void printmatrix(int mat[][3]){
for(int i=0;i<3;++i){
for(int j=0;j<3;++j){
printf("%d",mat[i][j]);
}
print("\n);
}
}
